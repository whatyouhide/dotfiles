# Make sure that the PHP install in MAMP is the first one
PATH=/Applications/MAMP/bin/php/php5.4.10/bin

PATH=$PATH:/usr/local/sbin
PATH=$PATH:/usr/local/bin
PATH=$PATH:/usr/bin
PATH=$PATH:/usr/sbin
PATH=$PATH:/bin
PATH=$PATH:/sbin
PATH=$PATH:/usr/local/git/bin

PATH=$PATH:$HOME/.rvm/bin

PATH=$PATH:/Applications/MAMP/Library/bin

# Personal scripts found in .dotfiles/bin and .dotfiles/bin/local
# (for personal use only - you can delete the entire directory content
# without affecting anything else)
PATH=$PATH:$HOME/.dotfiles/bin
PATH=$PATH:$HOME/.dotfiles/bin/local

export PATH
