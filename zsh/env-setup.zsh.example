# Contains basic environment variables setup that will be used by the whole
# config. This is sourced before everything else in ~/.zshenv.

# Path to the dotfiles.
export DOTFILES="$HOME/dotfiles"

# zsh config.
export ZSH_CONFIG="$DOTFILES/zsh"
export ZSH_PROMPT="freedom"

# Autostart tmux.
export AUTOSTART_TMUX=true
