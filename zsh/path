# Make sure that the PHP install in MAMP is the first one
export PATH=/Applications/MAMP/bin/php/php5.4.10/bin

export PATH=$PATH:/usr/local/sbin
export PATH=$PATH:/usr/local/bin
export PATH=$PATH:/usr/bin
export PATH=$PATH:/usr/sbin
export PATH=$PATH:/bin
export PATH=$PATH:/sbin

PATH=$PATH:$HOME/.rvm/bin

# Personal scripts found in $DOTFILES/bin and $DOTFILES/bin/local.
# (for personal use only - you can delete the entire directory content
# without affecting anything else)
export PATH=$PATH:$DOTFILES/bin

export PATH
